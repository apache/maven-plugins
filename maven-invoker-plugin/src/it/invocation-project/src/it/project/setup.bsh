import java.io.*;

String[] paths =
{
    "target",
    "sub-1/target",
    "sub-2/target",
    "sub-3/target",
};

for ( String path : paths )
{
    File dir = new File( basedir, path);
    System.out.println( "Creating directory: " + dir );
    if ( !dir.mkdirs() )
    {
        throw new IOException( "Failed to create directory: " + dir );
    }
}

return true;
