<?xml version="1.0"?>
<settings>
  <profiles>
    <profile>
      <id>apache</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>      
      <repositories>
        <repository>
          <id>local.central</id>
          <url>file://@localRepository@</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>             
        </repository>        
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>local.central</id>
          <url>file://@localRepository@</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>          
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
</settings>
