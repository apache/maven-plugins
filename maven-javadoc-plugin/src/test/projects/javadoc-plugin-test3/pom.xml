<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.apache.maven.plugin.javadoc.test3</groupId>
  <artifactId>javadoc-plugin-test3</artifactId>
  <packaging>jar</packaging>
  <version>1.0-SNAPSHOT</version>
  <inceptionYear>2006</inceptionYear>
  <name>Maven Javadoc Plugin Test3 for MJAVADOC-57</name>
  <url>http://maven.apache.org</url>
  <organization>
    <name>Apache Software Foundation</name>
    <url>http://www.apache.org/</url>
  </organization>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <developers>
    <developer>
      <id>vsiveton</id>
      <name>Vincent Siveton</name>
      <email>vsiveton@apache.org</email>
      <organization>Apache Software Foundation</organization>
      <roles>
        <role>Java Developer</role>
      </roles>
      <timezone>-5</timezone>
    </developer>
  </developers>
  <build>
	  <plugins>
		  <plugin>
			  <groupId>org.apache.maven.plugins</groupId>
			  <artifactId>maven-javadoc-plugin</artifactId>
			  <configuration>
				  <bottom>Copyright 2006 Brian S O'Neill. All Rights Reserved.</bottom>
			  </configuration>
		  </plugin>
	  </plugins>
  </build>
</project>
